{
    "name": "asset-transfer-basic",
    "version": "1.0.0",
    "description": "Asset Transfer Basic contract implemented in TypeScript",
    "main": "dist/index.js",
    "typings": "dist/index.d.ts",
    "engines": {
        "node": ">=12",
        "npm": ">=5"
    },
    "scripts": {
        "lint": "tslint -c tslint.json 'src/**/*.ts'",
        "pretest": "npm run lint",
        "start": "npm run build && node dist/app.js",
        "build": "tsc",
        "build:watch": "tsc -w",
        "prepublishOnly": "npm run build",
        "postinstall": "patch-package"
    },
    "engineStrict": true,
    "author": "Hyperledger",
    "license": "Apache-2.0",
    "dependencies": {
        "@daotl/cryptico": "^2.0.0",
        "@ont-dev/hdkey-secp256r1": "^1.1.2",
        "@types/elliptic": "^6.4.14",
        "cross-fetch": "^3.1.4",
        "cryptico": "^1.0.2",
        "debug": "^4.3.3",
        "ecdsa-secp256r1": "^1.3.3",
        "eckles": "^1.4.1",
        "ethers": "^5.5.2",
        "fabric-ca-client": "^2.2.4",
        "fabric-network": "^2.2.4",
        "jshashes": "^1.0.8",
        "node-fetch": "^3.1.0",
        "node-inspect": "^2.0.0",
        "superagent": "^6.1.0",
        "telnet-client": "^1.4.11"
    },
    "devDependencies": {
        "bip39": "^2.6.0",
        "elliptic": "^6.5.4",
        "jsrsasign": "^10.4.1",
        "patch-package": "^6.4.7",
        "tslint": "^5.11.0",
        "typescript": "^3.1.6"
    },
    "nyc": {
        "extension": [
            ".ts",
            ".tsx"
        ],
        "exclude": [
            "coverage/**",
            "dist/**"
        ],
        "reporter": [
            "text-summary",
            "html"
        ],
        "all": true,
        "check-coverage": true,
        "statements": 100,
        "branches": 100,
        "functions": 100,
        "lines": 100
    }
}
